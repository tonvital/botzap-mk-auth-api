{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off",
        "prettier/prettier": "error"
    },
    "plugins": [
        "prettier"
    ]
}